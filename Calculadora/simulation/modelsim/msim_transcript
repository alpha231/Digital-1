add wave -position end  sim:/calculadora/A
add wave -position end  sim:/calculadora/B
add wave -position end  sim:/calculadora/RES
add wave -position end  sim:/calculadora/NEG
add wave -position 2  sim:/calculadora/S
force -freeze sim:/calculadora/A 101 0
force -freeze sim:/calculadora/B 011 0
force -freeze sim:/calculadora/S 0 0
run
force -freeze sim:/calculadora/S 1 0
run
force -freeze sim:/calculadora/B 111 0
run
force -freeze sim:/calculadora/A 101 0
force -freeze sim:/calculadora/A 110 0
run
force -freeze sim:/calculadora/A 100 0
run
force -freeze sim:/calculadora/S 0 0
run
